{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://*.notion.so https://www.notion.so"
        },
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://www.notion.so"
        }
      ]
    }
  ]
}
